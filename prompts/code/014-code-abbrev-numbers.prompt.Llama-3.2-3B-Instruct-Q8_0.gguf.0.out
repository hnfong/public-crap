def compact_number(n):
    if n < 1000:
        return str(n)
    elif n < 1000000:
        return "{:.1f}k".format(n / 1000)
    elif n < 1000000000:
        return "{:.1f}M".format(n / 1000000)
    elif n < 1000000000000:
        return "{:.1f}B".format(n / 1000000000)
    else:
        return "{:.1f}T".format(n / 1000000000000) [end of text]


