<!DOCTYPE html><html><head><title>Poker</title></head><body><script>const d=[],s="23456789TJQKA",r="CDHS";for(i=0;i<52;i++)d.push(s[i%13]+r[i/13|0]);function sh(){for(i=d.length-1;i>0;i--){const j=Math.floor(Math.random()*(i+1));[d[i],d[j]]=[d[j],d[i]]}}function h(a){return d.splice(a,5)}function v(h){let c={},s=0;h.forEach(v=>{c[v[0]]||(c[v[0]]=0);c[v[0]]++});Object.values(c).sort((a,b)=>b-a).forEach((v,i)=>s+=v*(i+1));return s}sh();let p=h(0),d=h(0);document.body.innerHTML=`<h1>Poker</h1><p>Your hand: ${p.join(" ")}</p><p>Dealer: ${d.join(" ")}</p><p>${v(p)>v(d)?"You win!":"You lose!"}</p>`;</script></body></html> [end of text]


