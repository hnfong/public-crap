 <!DOCTYPE html>
 <html>
 <head>
 <title>Card Flip</title>
 <style>
 .card {
 display: inline-block;
 width: 100px;
 height: 150px;
 border: 1px solid black;
 margin: 10px;
 position: relative;
 }
 .card.flipped {
 transform: rotateY(180deg);
 }
 </style>
 </head>
 <body>
 <div id="game">
 <div class="card" data-value="1"></div>
 <div class="card" data-value="2"></div>
 <div class="card" data-value="3"></div>
 <div class="card" data-value="4"></div>
 <div class="card" data-value="5"></div>
 <div class="card" data-value="6"></div>
 <div class="card" data-value="7"></div>
 <div class="card" data-value="8"></div>
 <div class="card" data-value="9"></div>
 <div class="card" data-value="10"></div>
 </div>
 <script>
 const cards = document.querySelectorAll('.card');
 let flipped = [];
 let currentCard;
 let gameOver = false;
 let score = 0;
 function flipCard(card) {
 card.classList.add('flipped');
 flipped.push(card);
 if (flipped.length === 2) {
 if (flipped[0].dataset.value === flipped[1].dataset.value) {
 score++;
 console.log('Match found!');
 flipped = [];
 } else {
 console.log('No match found!');
 setTimeout(() => {
 card.classList.remove('flipped');
 flipped.shift();
 if (flipped.length === 0) {
 gameOver = true;
 console.log('Game over!');
 }
 }, 1000);
 }
 }
 function startGame() {
 cards.forEach(card => card.classList.remove('flipped'));
 flipped = [];
 gameOver = false;
 score = 0;
 currentCard = null;
 }
 document.addEventListener('DOMContentLoaded', startGame);
 document.querySelectorAll('.card').forEach(card => card.addEventListener('click', () => {
 if (!gameOver) {
 flipCard(card);
 }
 }));
 </script>
 </body>
 </html> [end of text]


