def compact_number(n):
    """
    Convert an integer to a compact, human-readable string representation.
    The output is less than 4 characters if possible.
    """
    if n == 0:
        return "0"
    
    # Define the units and their corresponding thresholds
    units = [
        (1_000_000_000_000, "T"),  # Trillion
        (1_000_000_000, "B"),      # Billion
        (1_000_000, "M"),          # Million
        (1_000, "k"),              # Thousand
        (1, "")                    # No unit (for numbers < 1000)
    ]
    
    # Handle negative numbers
    is_negative = n < 0
    n = abs(n)
    
    # Find the appropriate unit
    unit = ""
    for threshold, suffix in units:
        if n >= threshold:
            unit = suffix
            break
    
    # Convert to compact form with 1 or 2 significant digits
    if unit == "":
        # No unit, just the number
        if n < 10:
            return str(n)
        elif n < 100:
            return str(n)
