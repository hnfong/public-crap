def binary_search_file(file_path, target):
    with open(file_path, 'rb') as f:
        lines = f.readlines()
    lines = [int(line) + len(line.encode()) for line in lines]
    low, high = 0, len(lines) - 1
    while low <= high:
        mid = (low + high) // 2
        if lines[mid] == target:
            return mid
        elif lines[mid] < target:
            low = mid + 1
        else:
            high = mid - 1
    return low [end of text]


