import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        String input = scanner.nextLine();
        String[] parts = input.split(" and ");
        double total = 0;
        for (String part : parts) {
            String[] parts2 = part.split(", ");
            for (String p : parts2) {
                String[] parts3 = p.split(" ");
                double value = Double.parseDouble(parts3[0].replace(",", ""));
                String suffix = parts3[1].toUpperCase();
                switch (suffix) {
                    case "K":
                        total += value * 1024;
                        break;
                    case "M":
                        total += value * 1024 * 1024;
                        break;
                    case "G":
                        total += value * 1024 * 1024 * 1024;
                        break;
                    case "K":
                        total += value * 1024;
                        break;
                    case "M":
                        total += value * 1024 * 1024;
                        break;
                    case "G":
                        total += value * 1024 * 1024 * 1024;
                        break;
                }
            }
        }
        System.out.printf("%.1f%n", total);
        System.out.printf("%.1f K%n", total / 1024);
        System.out.printf("%.1f M%n", total / (1024 * 1024));
        System.out.printf("%.1f G%n", total / (1024 * 1024 * 1024));
    }
} [end of text]


