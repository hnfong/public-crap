        "https://via.placeholder.com/100x150?text=63",
        "https://via.placeholder.com/100x150?text=64"
      ];
      const deck = document.querySelector('.deck');
      const discard = document.querySelector('.discard');
      const shuffle = () => {
        for (let i = cards.length - 1; i > 0; i--) {
          const j = Math.floor(Math.random() * (i + 1));
          [cards[i], cards[j]] = [cards[j], cards[i]];
        }
        cards.forEach((card, index) => {
          const cardElement = document.createElement('div');
          cardElement.classList.add('card');
          cardElement.id = `card${index + 1}`;
          cardElement.innerHTML = `<img src="${card}" alt="Card ${index + 1}">`;
          deck.appendChild(cardElement);
        });
      };
      shuffle();
      const moveCard = (from, to) => {
        const card = document.getElementById(from);
        const target = document.getElementById(to);
        card.parentNode.removeChild(card);
        target.parentNode.insertBefore(card, target);
      };
      const checkForMatch = () => {
        const cardsInDiscard = discard.querySelectorAll('.card');
        const lastCard = cardsInDiscard[cardsInDiscard.length - 1];
        if (lastCard) {
          const lastCardId = lastCard.id;
          const lastCardValue = lastCard.querySelector('img').alt;
          const lastCardIndex = parseInt(lastCardId.slice(4)) - 1;
          const lastCardValueIndex = lastCardValue - 1;
          const lastCardValueInDeck = cards[lastCardValueIndex];
          const lastCardValueInDiscard = cards[lastCardValueInDeck - 1];
          if (lastCardValueInDiscard === lastCardValueInDeck) {
            moveCard(lastCardId, 'card1');
            shuffle();
          }
        }
      };
      setInterval(checkForMatch, 1000);
