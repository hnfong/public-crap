        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        height: 100vh;
        margin: 0;
        background-color: #f0f0f0;
      }
      .card {
        width: 100px;
        height: 150px;
        margin: 10px;
        background-color: #fff;
        border: 2px solid #ccc;
        border-radius: 10px;
        display: flex;
        align-items: center;
        justify-content: center;
        box-shadow: 0 4px 8px rgba(0,0,0,0.1);
      }
      .card img {
        width: 100%;
        height: 100%;
        object-fit: cover;
      }
      .deck {
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
      }
      .discard {
        display: flex;
        flex-direction: column;
        align-items: center;
        margin-top: 20px;
      }
      .discard img {
        width: 100px;
        height: 150px;
        object-fit: cover;
      }
    </style>
