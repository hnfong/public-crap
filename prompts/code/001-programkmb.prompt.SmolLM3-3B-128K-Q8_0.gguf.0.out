import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class NumberSummer {
    public static void main(String[] args) {
        String input = System.console().readLine();
        String[] lines = input.split("\\n");
        double total = 0.0;

        for (String line : lines) {
            Matcher matcher = Pattern.compile("([0-9,.]+)([kKmMgG])?").matcher(line);
            while (matcher.find()) {
                String number = matcher.group(1);
                String suffix = matcher.group(3);

                double value = parseNumber(number);
                if (suffix != null) {
                    switch (suffix.toLowerCase()) {
                        case "k":
                            value *= 1024;
                            break;
                        case "m":
                            value *= 1024 * 1024;
                            break;
                        case "g":
                            value *= 1024 * 1024 * 1024;
                            break;
                        case "k":
                            value *= 1024;
                            break;
                        case "M":
                            value *= 1024 * 1024;
                            break;
                        case "G":
                            value *= 1024 * 1024 * 1024;
                            break;
                    }
                }
                total += value;
            }
        }

        System.out.println(String.format("%.1f", total));
        System.out.println(String.format("%.1f k", total / 1024));
        System.out.println(String.format("%.1f M", total / (1024 * 1024)));
        System.out.println(String.format("%.1f G", total / (1024 * 1024 * 1024)));
    }

    private static double parseNumber(String number) {
        number = number.replaceAll(",", "").replace("-", "");
        return Double.parseDouble(number);
    }
}
