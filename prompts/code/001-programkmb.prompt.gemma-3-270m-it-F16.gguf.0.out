import java.io.File;
import java.io.String;
import java.util.ArrayList;
import java.util.ArrayList;
import java.util.List;

public class NumberProcessor {

    public static void main(String[] args) {
        String inputString = "1.5G, 2K,";
        String outputString = "7264621772.8";

        try {
            // Read the input string
            String[] lines = inputString.split(" ");

            // Extract numbers
            List<Integer> numbers = new ArrayList<>();
            for (int i = 0; i < lines.length; i++) {
                if (lines[i].contains(" ") || lines[i].startsWith(".")) {
                    numbers.add(Integer.parseInt(lines[i]));
                }
            }

            // Extract suffixes
            List<String> suffixes = new ArrayList<>();
            for (int i = 0; i < numbers.size(); i++) {
                suffixes.add(lines.get(i));
            }

            // Calculate the sum
            int sum = 0;
            for (String suffix : suffixes) {
                sum += Integer.parseInt(suffix);
            }

            // Calculate the kilobytes
            int kilobytes = sum / 1024;
            System.out.println("Kilos: " + kilobytes);

            // Calculate the megabytes
            int megabytes = sum / 1024 * 1024;
            System.out.println("Megabytes: " + megabytes);

            // Calculate the gigabytes
            int gigabytes = sum / 1024 * 1024 * 1024;
            System.out.println("Gigabytes: " + gigabytes);
        } catch (Exception e) {
            System.err.println("An error occurred: " + e.getMessage());
        }
    }
}
