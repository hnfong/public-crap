def abbrev_num(x):
    if x < 1000:
        return str(x)
    if x < 1000000:
        return f"{x // 1000}K"
    if x < 1000000000:
        return f"{x // 1000000}M"
    if x < 1000000000000:
        return f"{x // 1000000000}G"
    return f"{x // 1000000000000}T"
